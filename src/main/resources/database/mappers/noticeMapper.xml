<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dg.s8.notice.NoticeDAO">

   <select id="getSelect" parameterType="com.dg.s8.notice.NoticeDTO" resultType="com.dg.s8.notice.NoticeDTO">
      SELECT * FROM NOTICE WHERE NUM=#{num}
   </select>
   
   <select id="getList" resultType="com.dg.s8.notice.NoticeDTO">
      SELECT * FROM NOTICE
   </select>
   
   <insert id="setInsert" parameterType="com.dg.s8.notice.NoticeDTO">
  		INSERT INTO NOTICE (num, title, contents, writer)
  		VALUES (notice_seq.nextval, #{title}, #{contents}, #{writer})
  	</insert>
  	
  	<delete id="setDelete" parameterType="java.lang.Long">
  		DELETE NOTICE WHERE NUM=#{num}
  	</delete>
  	
   
   
</mapper>